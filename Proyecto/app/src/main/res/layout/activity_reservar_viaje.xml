        android:layout_marginEnd="92dp"
        android:text="Precio"
        android:textColor="#FFFFFF"
        app:layout_constraintEnd_toStartOf="@+id/text_precio"
        app:layout_constraintTop_toBottomOf="@+id/textView14" />
